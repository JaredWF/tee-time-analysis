{
  "name": "tee-times",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start-db": "ts-node ./src/db/db.ts",
    "migrate-db": "ts-node ./src/db/migrate.ts",
    "generate-db-schema": "kysely-codegen --dialect sqlite",
    "start-graphql": "ts-node ./src/graphql/index.ts",
    "generate-gql-types": "graphql-codegen --config codegen.ts",
    "scrape": "ts-node -r tsconfig-paths/register ./src/scrapers/index.ts",
    "test": "jest",
    "start-all": "npm-run-all --parallel start-graphql scrape"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@apollo/server": "^4.8.1",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "axios": "1.4.0",
    "better-sqlite3": "^8.4.0",
    "cross-fetch": "4.0.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "graphql": "^16.7.1",
    "kysely": "^0.26.1",
    "next": "13.4.10",
    "npm": "^9.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.7.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-typescript": "^7.22.11",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/better-sqlite3": "^7.6.4",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.6.4",
    "jest": "^29.6.4",
    "kysely-codegen": "^0.10.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}